<div>
    <div class="card bg-light m-3" style="width: 18rem;">
        <div class="card-header h5">{{this.name}}</div>
        <img class="card-img-top myImageHeight" src="{{this.image_url}}" alt="Card image cap">
        <div class="card-body">
                <p class="card-text">{{this.description}}</p>
                <p class="card-text text-secondary font-italic">{{this.tagline}}</p>
        </div>
    </div>
</div>